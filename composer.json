{
    "name": "rizalmovic/cms",
    "description": "CMS package",
    "keywords": [
        "rizalmovic",
        "laravel",
        "cms"
    ],
    "homepage": "https://github.com/rizalmovic/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "rizalmovic",
            "email": "rizalmovic@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "inertiajs/inertia-laravel": "^0.4.4",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Rizalmovic\\Cms\\": "src",
            "Rizalmovic\\Cms\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rizalmovic\\Cms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rizalmovic\\Cms\\CmsServiceProvider"
            ],
            "aliases": {
                "Cms": "Rizalmovic\\Cms\\CmsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
